{
  "builds": [
    {
      "src": "api/assistant.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/assistant.py"
    }
  ]
}
{
  "builds": [
    {
      "src": "api/assistant.py",
      "use": "@vercel/python",
      "config": {
        "pythonVersion": "3.10"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/assistant.py"
    }
  ]
}
