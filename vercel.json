{
  "builds": [
    {
      "src": "api/assistant.py",
      "use": "@vercel/python",
      "config": {
        "pythonVersion": "3.10"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/assistant.py"
    },
    {
      "src": "/",
      "dest": "public/index.html"
    },
    {
      "src": "/style.css",
      "dest": "public/style.css"
    },
    {
      "src": "/script.js",
      "dest": "public/script.js"
    },
    {
      "src": "/(.*)",
      "dest": "public/index.html"
    }
  ]
}
